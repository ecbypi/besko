<button class="prev">Previous Day</button>
<button class="next">Next Day</button>

<h2><%= @date.getUTCDayName() %>, <%= @date.getUTCMonthName() %> <%= @date.getUTCDate() %>, <%= @date.getUTCFullYear() %></h2>

<table data-collection="deliveries">
