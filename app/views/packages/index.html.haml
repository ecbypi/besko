%table{data: { collection: :packages }}
  %tr
    %th Received By
    %th Received At
    %th Delivered By
    %th Comment
    %th
  - collection.each do |package|
    %tr{data: { 'resource-id' => package.id }}
      %td= package.mail_to_worker
      %td= package.received_at
      %td= package.delivered_by
      %td= package.comment
      %td= package.sign_out_button
